{
  "name": "biar-fca",
  "version": "3.7.7",
  "module": "commonjs",
  "types": "src/types/index.d.ts",
  "description": "Advanced Facebook Chat API (FCA) library with built-in anti-detection protection and automatic cookie refresh. Pure npm package for building reliable Messenger bots - forked from ws3-fca",
  "main": "module/index.js",
  "files": [
    "module/",
    "src/",
    "LICENSE-MIT",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "test": "mocha tests/**/*.test.js --timeout 10000 --exit",
    "test:watch": "mocha tests/**/*.test.js --watch",
    "test:coverage": "nyc --reporter=text --reporter=html mocha tests/**/*.test.js --timeout 10000 --exit",
    "lint": "eslint src/**/*.js module/**/*.js",
    "lint:fix": "eslint src/**/*.js module/**/*.js --fix",
    "format": "prettier --write \"src/**/*.js\" \"module/**/*.js\" \"tests/**/*.js\"",
    "format:check": "prettier --check \"src/**/*.js\" \"module/**/*.js\" \"tests/**/*.js\"",
    "audit": "npm audit",
    "audit:fix": "npm audit fix",
    "prepublishOnly": "echo 'Preparing to publish biar-fca...'",
    "docs": "jsdoc -c jsdoc.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Jubiar01/biar-fca"
  },
